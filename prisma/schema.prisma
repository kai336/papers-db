datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Paper {
  id        Int      @id @default(autoincrement())
  title     String
  author    String
  year      Int
  tags      String
  summary   String
  pdfPath   String
  createdAt DateTime @default(now())
}
